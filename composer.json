{
	"name": "flame/module-installer",
	"type": "composer-installer",
	"description": "Custom installer for Nette modules",
	"keywords": ["nette", "installer", "modules"],
	"homepage": "https://github.com/flame-org/Nette-Module-Installer",
	"license": ["BSD-3", "GPLv2", "GPLv3"],
	"authors": [
		{
			"name": "Jiří Šifalda",
			"homepage": "http://jsifalda.name/"
		}
	],
	"require": {
		"php": ">= 5.3.0"
	},
	"autoload": {
		"psr-0": {
      "Flame": "",
      "Nette": "libs/"
    }
	},
	"extra": {
		"class": "Flame\\Bundles\\Installer"
	},
	"require-dev": {
		"composer/composer": "dev-master"
	}
}
