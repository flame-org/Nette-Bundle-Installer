{
	"name": "flame/bundle-installer",
	"type": "composer-installer",
	"description": "Custom installer for Nette Bundles",
	"keywords": ["nette", "installer", "bundles"],
	"homepage": "https://github.com/flame-org/nette-bundle-installer",
	"license": ["BSD-3", "GPLv2", "GPLv3"],
	"authors": [
		{
			"name": "Jiří Šifalda",
			"homepage": "http://jsifalda.name/"
		}
	],
	"require": {
		"php": ">= 5.3.0",
		"nette/nette": ">=2.0.1"
	},
	"autoload": {
		"psr-0": { "Flame": "" }
	},
	"extra": {
		"class": "Flame\\Bundles\\Installer"
	},
	"require-dev": {
		"composer/composer": "dev-master"
	}
}
