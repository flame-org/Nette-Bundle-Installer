{
	"name": "flame/budle-installer",
	"type": "composer-installer",
	"description": "Custom installer for Nette Bundles",
	"keywords": ["nette", "installer", "bundles"],
	"homepage": "https://github.com/flame-org/nette-budle-installer",
	"license": ["BSD-3", "GPLv2", "GPLv3"],
	"authors": [
		{
			"name": "Jan Dolecek",
			"homepage": "http://juzna.cz"
		},
		{
			"name": "Nette Community",
			"homepage": "https://github.com/nette/nette/contributors"
		}
	],
	"require": {
		"php": ">= 5.3.0",
		"nette/nette": ">=2.0.1"
	},
	"autoload": {
		"psr-0": { "Nette\\Addons": "src/" }
	},
	"extra": {
		"class": "Nette\\Addons\\Installer"
	},
	"require-dev": {
		"composer/composer": "1.0.*"
	},
	"minimum-stability": "dev"
}
